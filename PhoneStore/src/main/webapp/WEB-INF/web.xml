<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>test</servlet-name>
        <servlet-class>MobileStore.Servlet.Admin.test</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>MobileStore.Servlet.Admin.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminProductServlet</servlet-name>
        <servlet-class>MobileStore.Servlet.Admin.AdminProductServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminDiscountServlet</servlet-name>
        <servlet-class>MobileStore.Servlet.Admin.AdminDiscountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminAccountServlet</servlet-name>
        <servlet-class>MobileStore.Servlet.Admin.AdminAccountServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminInvoiceServlet</servlet-name>
        <servlet-class>MobileStore.Servlet.Admin.AdminInvoiceServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>StatisticsServlet</servlet-name>
        <servlet-class>MobileStore.Servlet.Admin.StatisticsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CustomerProductServlet</servlet-name>
        <servlet-class>MobileStore.Servlet.Customer.CustomerProductServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Customer</servlet-name>
        <servlet-class>MobileStore.Servlet.Customer.CustomerServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>test</servlet-name>
        <url-pattern>/test</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/Admin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminProductServlet</servlet-name>
        <url-pattern>/AdminProduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminDiscountServlet</servlet-name>
        <url-pattern>/AdminDiscount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminAccountServlet</servlet-name>
        <url-pattern>/AdminAccount</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminInvoiceServlet</servlet-name>
        <url-pattern>/AdminInvoice</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>StatisticsServlet</servlet-name>
        <url-pattern>/Statistics</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CustomerProductServlet</servlet-name>
        <url-pattern>/CustomerProduct</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Customer</servlet-name>
        <url-pattern>/CustomerServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
